{
  "version": 2,
  "projects": {
    "i18n-react": {
      "root": "packages/i18n/react",
      "sourceRoot": "packages/i18n/react/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/i18n/react/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/i18n/react",
            "tsConfig": "packages/i18n/react/tsconfig.lib.json",
            "project": "packages/i18n/react/package.json",
            "entryFile": "packages/i18n/react/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/i18n/react"],
          "options": {
            "jestConfig": "packages/i18n/react/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "i18n-core": {
      "root": "packages/i18n/core",
      "sourceRoot": "packages/i18n/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["packages/i18n/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/i18n/core"],
          "options": {
            "jestConfig": "packages/i18n/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/i18n/core",
            "tsConfig": "packages/i18n/core/tsconfig.lib.json",
            "packageJson": "packages/i18n/core/package.json",
            "main": "packages/i18n/core/src/index.ts",
            "assets": [
              "packages/i18n/core/*.md",
              {
                "input": "./packages/i18n/core/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/i18n/core",
                "glob": "generators.json",
                "output": "."
              },
              {
                "input": "./packages/i18n/core",
                "glob": "executors.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "i18n-core-e2e": {
      "projectType": "application",
      "root": "packages\\i18n\\core-e2e",
      "sourceRoot": "packages\\i18n\\core-e2e/src",
      "targets": {
        "e2e": {
          "executor": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "i18n-core:build",
            "npmPackageName": "@nx-plugins/core",
            "pluginOutputPath": "dist/packages/i18n/core",
            "jestConfig": "packages/i18n/core-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
